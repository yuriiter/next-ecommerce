{
  "name": "next-ecommerce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "next:dev": "next dev",
    "db:dev": "docker-compose down; docker-compose up --remove-orphans db 0>&1> /dev/null",
    "express:dev": "NODE_ENV=development pnpm run --prefix ./server dev",
    "docker:reset": "docker-compose down -v; docker-compose rm -f",
    "dev": "pnpx concurrently \"pnpm run next:dev\" \"pnpm run express:dev\" \"pnpm run db:dev\" ",
    "build": "docker-compose up -d",
    "start": "next start"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "next": "13.4.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^9.0.1",
    "react-ranger": "^2.1.0",
    "remark-gfm": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "20.4.10",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/react-ranger": "^2.0.4",
    "concurrently": "^8.2.2",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.13",
    "sass": "^1.66.1",
    "typescript": "5.1.6"
  }
}
